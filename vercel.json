{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/room/:modelname", "destination": "/room.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/app", "destination": "/app.html" },
    { "source": "/landing.html", "destination": "/landing.html" },
    { "source": "/afterhours", "destination": "/index-afterhours.html" }
  ],
  "routes": [
    {
      "src": "/room.html",
      "dest": "/room.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/dashboard.html"
    },
    {
      "src": "/app.html",
      "dest": "/app.html"
    },
    {
      "src": "/landing.html",
      "dest": "/landing.html"
    },
    {
      "src": "/index-afterhours.html",
      "dest": "/index-afterhours.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
